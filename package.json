{
  "name": "node-typescript-template",
  "version": "1.0.0",
  "description": "A template for an empty node.js project with TypeScript",
  "main": "index.ts",
  "scripts": {
    "lint": "eslint --fix",
    "run": "esr --cache src/index.ts",
    "dev": "esbuild-dev -w",
    "build": "rm -rf dist && esbuild src/index.ts --bundle --platform=node --target=node16.14.2 --outdir=dist",
    "start": "node dist/index.js"
  },
  "author": "Konstantin Spie√ü",
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "esbuild": "^0.14.46",
    "esbuild-dev": "^0.10.0",
    "esbuild-runner": "^2.2.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "archiver-zip-encrypted": "^1.0.11",
    "aws-sdk": "^2.1159.0",
    "dotenv": "^16.0.1"
  }
}
